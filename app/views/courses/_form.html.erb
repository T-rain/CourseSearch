<%= form_for @course do |f| %>

      <table>
        <tr>
          <td>課程名稱:</td>
          <td><%= f.text_field :course %></td>
        </tr>
        <tr>
          <td>時間:</td>
          <td><%= f.text_field :time%></td>
        </tr>
        <tr>
          <td>老師:</td>
          <td><%= f.text_field :teacher%></td>
        </tr>
      </table>

      <%= f.submit '提交'%>
      

<% end %>
